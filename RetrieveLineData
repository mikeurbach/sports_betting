<script type="text/javascript">
    
    var vm = new ViewModel(sports, wcmSports);
    vm.start();
    $(document).bind('pageinit', function () {
        ko.applyBindings(vm);
    });

    function GoLeagueSelected(s, l, rn) {
        var url = "/sports/offerings?s=" + s + "&l=" + l + "&p=" + 0 + "&rn=" + rn;
        window.location.replace(url);
		mostPopularRefresh();
    };

    function PromotionComeBack() {

        $("#home-view").css({ 'background-color': '#DFDFDF' });

        //Hide Promotion Details
        $("#promo-desc").empty();
        $("#promotion-come-back-home").hide();
        $("#promo-desc").hide();
        $("#term-promotion").hide();

        //Show Leagues
        $("#sports-content").show();
        $("#page").show();

        //Show Most Popular
        if($("#most-popular-content").html() != ""){
            $("#most-popular-content").show();
        }
        $("#promo-list").show();
        
        if ($("#race-popular-content").html() != "") {
            $("#race-popular-content").show();
        }
   
        if ($("#casino-popular-content").html() != "") {
            $("#casino-popular-content").show();
        }
        
   
    }

    function GetTermsPromotion() {
        var key = nameterms;

        $.ajax({
            url: '/content/HtmlContent',
            type: 'GET',
            data: { key: key },
            success: function (data) {

                //Hide Promotion Details
                $("#promotion-description-come-back").show();
                $("#promo-desc").append("");
                $("#promotion-come-back-home").hide();
                $("#promo-desc").hide();
                $("#term-promotion").hide();

                //Show Terms
                $("#promo-terms").append(data);
                $("#promo-terms").show();

            }
        });
    }

    function PromotionDescriptionComeBack() {
        // Show Promotion Details
        $("#promotion-description-come-back").hide();
        $("#home-view").css({ 'background-color': '#fff' });
        $("#promotion-come-back-home").show();
        $("#promo-desc").show();
        $("#term-promotion").show();

        // Hide terms
        $("#promo-terms").hide();
        $("#promo-terms").empty();
    }

    var countSport = availableSportsSorted.length;
    var owl = $("#owl-demo");
    owl.owlCarousel({
        items: 4,
        jsonData: availableSportsSorted,
        jsonPath: false,
        jsonSuccess: customDataSuccess,
        itemsDesktop: [1000, 4], //4 items between 1000px and 901px
        itemsDesktopSmall: [900, 4], // betweem 900px and 601px
        itemsTablet: [600, 4],
        itemsMobile: [480, 4]
    });

    // Custom Navigation Events
    $(".next").click(function () {
        owl.trigger('owl.next');
    })

    $(".prev").click(function () {
        owl.trigger('owl.prev');
    })

    function customDataSuccess(data) {
        for (i = 0; i <= countSport - 1; i++) {
            var sportimage = data[i] + ".png";
            var staticdomain = "http://mobile2.sportsbetting.ag/ui";
                var siteid = "2";
                var pathimage = staticdomain + "/Content/mobile/" + siteid + "/img/icons/" + sportimage;
                $("#owl-demo").append(
                    $('<div>').append(
                        $('<div>', {
                            class: 'item',
                            'data-theme': 'a'
                        }).append(
                        $('<a>', {
                            href: '#',
                            'data-bind': 'click:GotoSports'
                        }).append(
                        $('<img>', {
                            name: data[i],
                            src: pathimage,
                            width: 48,
                            height: 48
                        }))),
                        $('<div>', {
                            class: 'itemname',
                            'data-theme': 'a'
                        }).append(data[i])
                    )
                 );
            }
        }

</script>


<script type="text/javascript">

    function redirectTo(url) {
        window.location = url;
    }

    function joinBackHistory() {
        history.back();
        return false;
    }

    function joinPostBackHistory(formID, url) {
        $(formID).attr("action", url);
        $(formID).submit();
    }

</script>

