	    function hideLoadingPopupMessage(delay) {
	        if (delay == true) {
	            setTimeout(function () {
	                $.mobile.loading('hide');
	            }, 500);
	        }
	        else {
	            $.mobile.loading('hide');
	        }
	    }
	    function hideLoadingDivMessage(delay) {
	        if (delay == true) {
	            setTimeout(function () {
	                $("#ui-loader-message").hide();
	                $("#straightPopup-screen").remove();

	            }, 500);
	        }
	        else {
	            $("#ui-loader-message").hide();
	            $("#straightPopup-screen").remove();
	        }
	    }
	</script>

