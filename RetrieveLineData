    function mostPopularRefresh() {

        try {
            showMostPopular();
        } catch (err) { }

        setTimeout("mostPopularRefresh()", 950000)
    }

    function showMostPopular() {
        var key = "mostpopular";
        var nElements = 7;
        var seconds = 60;

        var url = "/home/most-popular-widgets?key=" + key + "&nElements=" + nElements + "&seconds=" + seconds;
        $("#most-popular-content").load(url);
    }
