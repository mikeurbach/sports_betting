<script>
    function ShowDetailPromotion(name, desc, term) {

        // Set name of promotions
        nameterms = term;

        // Hide Leagues
        $("#sports-content").hide();
        //$("#page").hide();

        // Hide Most Popular
        $("#most-popular-content").hide();
        $("#race-popular-content").hide();
        $("#casino-popular-content").hide();
        $("#promo-list").hide();

        //Show Promotios Details
        $("#home-view").css({ 'background-color': '#fff' });
        $(".promo-details .ui-btn-inner .ui-btn-text").html(name);
        $("#promo-desc").append(desc);
        $("#promotion-come-back-home").show();
        $("#promo-desc").show();
        $("#term-promotion").show();
    }
</script>
 
